require ["duplicate","fileinto"];
# rule:[test-duplicate]
if duplicate
{
	fileinto "urgent";
}
# rule:[test-duplicate-2]
if allof (duplicate :handle "support" :header "X-Ticket-ID", header :contains "subject" "fileserver")
{
	fileinto "test";
}
# rule:[test-duplicate-3]
if not duplicate :uniqueid "test" :seconds 1800
{
	discard;
}
